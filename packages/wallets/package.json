{
  "name": "@web3sdkio/wallets",
  "version": "0.1.0",
  "main": "dist/web3sdkio-wallets.cjs.js",
  "module": "dist/web3sdkio-wallets.esm.js",
  "exports": {
    ".": {
      "module": "./dist/web3sdkio-wallets.esm.js",
      "default": "./dist/web3sdkio-wallets.cjs.js"
    },
    "./wallets/base": {
      "module": "./wallets/base/dist/web3sdkio-wallets-wallets-base.esm.js",
      "default": "./wallets/base/dist/web3sdkio-wallets-wallets-base.cjs.js"
    },
    "./wallets/injected": {
      "module": "./wallets/injected/dist/web3sdkio-wallets-wallets-injected.esm.js",
      "default": "./wallets/injected/dist/web3sdkio-wallets-wallets-injected.cjs.js"
    },
    "./wallets/metamask": {
      "module": "./wallets/metamask/dist/web3sdkio-wallets-wallets-metamask.esm.js",
      "default": "./wallets/metamask/dist/web3sdkio-wallets-wallets-metamask.cjs.js"
    },
    "./wallets/wallet-connect": {
      "module": "./wallets/wallet-connect/dist/web3sdkio-wallets-wallets-wallet-connect.esm.js",
      "default": "./wallets/wallet-connect/dist/web3sdkio-wallets-wallets-wallet-connect.cjs.js"
    },
    "./wallets/coinbase-wallet": {
      "module": "./wallets/coinbase-wallet/dist/web3sdkio-wallets-wallets-coinbase-wallet.esm.js",
      "default": "./wallets/coinbase-wallet/dist/web3sdkio-wallets-wallets-coinbase-wallet.cjs.js"
    },
    "./connectors/injected": {
      "module": "./connectors/injected/dist/web3sdkio-wallets-connectors-injected.esm.js",
      "default": "./connectors/injected/dist/web3sdkio-wallets-connectors-injected.cjs.js"
    },
    "./connectors/metamask": {
      "module": "./connectors/metamask/dist/web3sdkio-wallets-connectors-metamask.esm.js",
      "default": "./connectors/metamask/dist/web3sdkio-wallets-connectors-metamask.cjs.js"
    },
    "./connectors/wallet-connect": {
      "module": "./connectors/wallet-connect/dist/web3sdkio-wallets-connectors-wallet-connect.esm.js",
      "default": "./connectors/wallet-connect/dist/web3sdkio-wallets-connectors-wallet-connect.cjs.js"
    },
    "./connectors/coinbase-wallet": {
      "module": "./connectors/coinbase-wallet/dist/web3sdkio-wallets-connectors-coinbase-wallet.esm.js",
      "default": "./connectors/coinbase-wallet/dist/web3sdkio-wallets-connectors-coinbase-wallet.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "repository": "https://github.com/web3sdkio/web3/tree/main/packages/wallets",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3sdkio/web3/issues"
  },
  "author": "web3sdkio eng <eng@web3sdk.io>",
  "files": [
    "dist/",
    "wallets/",
    "connectors/"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "connectors/**",
      "wallets/**"
    ],
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "exports": true
    },
    "exports": true
  },
  "sideEffects": false,
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.6.0",
    "@wagmi/core": "^0.7.6",
    "@walletconnect/ethereum-provider": "^1.7.8",
    "abitype": "^0.2.5",
    "ethers": "^5.7.2",
    "eventemitter3": "^5.0.0",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "@preconstruct/cli": "^2.2.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/",
    "fix": "eslint src/ --fix",
    "clean": "rm -rf dist/",
    "build": "tsc && preconstruct build"
  }
}
